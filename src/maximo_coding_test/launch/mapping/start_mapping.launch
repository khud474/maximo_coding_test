<launch>
  <arg name="world_name" default="$(find maximo_coding_test)/world/final_world.world" />
  <arg name="model" default="waffle" />

  <!-- Launch turtlebot3 bringup -->
  <include file="$(find maximo_coding_test)/launch/mapping/bringup.launch">
    <arg name="world_name" value="$(arg world_name)" />
    <arg name="model" value="$(arg model)" />
    <arg name="rviz_config" value="$(find maximo_coding_test)/rviz/mapping.rviz" />
  </include>

  <!-- Start mapping -->
  <include file="$(find turtlebot3_slam)/launch/turtlebot3_gmapping.launch">
    <arg name="model" value="$(arg model)" />
  </include>
</launch>
